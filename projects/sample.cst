import lib

# Simple circuit demonstration
v3v3 = supply("3V3")
gnd = dgnd()

def simple_circuit():
    # Basic LED circuit with current limiting resistor
    at v3v3
    wire down 100 right 100
    add res(220)
    wire right 100 down 100
    add led("green")
    wire down 100
    to gnd

# Create the main circuit
simple_circuit()
